{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error",4],
        "quotes": ["off"],
        "eol-last": ["off"],
        "no-console": ["off"],
        "comma-dangle": ["warn", "never"]
    }
}